{
  "name": "plymouth",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create --timestamp=true ./src/shared/database/migrations/${npm_config_name}",
    "migrate": "ts-node ./node_modules/typeorm/cli.js migration:run -d ormconfig.ts",
    "copy-assets": "mkdir -p ./dist/src/config && mkdir -p ./dist/src/i18n && cp -rpv ./src/assets/ ./dist/assets/ && cp -rpv ./src/config/*.yml ./dist/src/config/ && cp -rpv ./src/i18n/ ./dist/src/i18n/",
    "prebuild": "rimraf dist",
    "build": "npm run copy-assets && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"src/test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run build && npm run copy-assets && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:debug:webstorm": "npm run copy-assets && node -r ts-node/register -r tsconfig-paths/register src/main.ts",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test-local": "NODE_ENV=development jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.637.0",
    "@aws-sdk/client-dynamodb": "^3.637.0",
    "@aws-sdk/client-s3": "^3.637.0",
    "@fastify/cookie": "^9.4.0",
    "@fastify/helmet": "^11.1.1",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-fastify": "^10.4.1",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "async-mutex": "^0.5.0",
    "aws-sdk-v3-nest": "^0.3.0",
    "cache-manager": "^5.7.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "nanoid": "^3.3.4",
    "nestjs-dynamoose": "^0.5.8",
    "nestjs-i18n": "^10.4.5",
    "nestjs-pino": "^4.1.0",
    "nestjs-request-context": "^3.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.12.0",
    "pg-native": "^3.1.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.7",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.5",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
