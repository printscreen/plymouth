root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.{yml,yaml}]
indent_size = 4

[.sequelizerc]
file_type = javascript

[*.crt]
insert_final_newline = false

[*.pem]
insert_final_newline = false

[*.token]
insert_final_newline = false
